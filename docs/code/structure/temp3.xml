<?xml version="1.0" encoding="UTF-8"?>

<recipe>

  <default>
    <!-- 24 hours -->
    <cycle duration="86400">

      <!-- Day Time, 18 hours -->
      <period start="0" end="64799">
        <air>
          <temperature min="18" max="24" />
        </air>
      </period>

      <!-- Night Time, 6 hours -->
      <period start="64800" end="86399">
        <air>
          <temperature min="15" max="17" />
        </air>
      </period>
      
    </cycle>
  </default>
  
  <germination>
    <air>
      <temperature min="26" max="32" />
    </air>
  </germination>

</recipe>
